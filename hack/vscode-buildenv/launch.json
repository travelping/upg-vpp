{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "gdb",
      "request": "attach",
      "name": "Attach to gdbserver vpp",
      "executable": "/usr/bin/vpp",
      "target": "localhost:7777",
      "remote": true,
      "cwd": "${workspaceRoot}",
      "valuesFormatting": "parseText",
      "gdbpath": "./hack/attach-gdb.sh",
      "preLaunchTask": "Run e2e tests",
    }
  ]
}