{
  // for use with remote VSCode inside Docker container
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/**",
        "/src/vpp-base/vpp/**"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/clang-11",
      "cStandard": "c17",
      "cppStandard": "c++14",
      "intelliSenseMode": "linux-clang-x64",
      "configurationProvider": "ms-vscode.makefile-tools"
    }
  ],
  "version": 4
}
